#content .h_banner{
	width: 100%;
	position: relative;
}
#content .h_banner .bannerbox{
	position: relative;
	width: 100%;
	height: 360px;
	background: #8d02de;
}
#content .h_banner .bannerbox div{
	position: absolute;
	left: 50%;
	top: 50%;
	width: 770px;
	height: 360px;
	margin-top: -180px;
	margin-left: -335px;
	display: none;
}
#content .h_banner .bannerbox .bannerf{
	display: block;
}
#content .h_banner .bannerbox ul{
	width: 240px;
	position: absolute;
	bottom: 15px;
	left: 50%;
	margin-left: -70px ;
}
#content .h_banner .bannerbox ul li{
	float: left;
	width: 20px;
	height: 20px;
	display: block;
	background: #999;
	cursor: pointer;
	border-radius: 50%;
	margin-left: 10px;
	text-align: center;
	line-height: 20px;
	color: #fff;
}
#content .h_banner .banner-rightbox{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 0;
}
#content .h_banner .Center .banner-right{
	
	float:right;
	width: 198px;
	height: 358px;
	background: #fff;
	z-index: 100000;
	overflow: hidden;
}
#content .h_banner .Center .banner-right .br{
	border-bottom: 1px dotted #ddd;
}
#content .h_banner .Center .banner-right div img{
	margin-left: 0;
}
#content .Center{
	overflow: hidden;
}
#content .jingxi{
	margin-top: 30px;
}
#content .jingxi .jingxi-left{
	float: left;
	width: 328px;
    height: 460px;
    background: #ff827f;
}
#content .jingxi .jingxi-left img{
	display: inline-block;
}
#content .jingxi .jingxi-left .jingxi-box{
	margin: 30px;
}
#content .jingxi .jingxi-left .jingxi-box .left-top{
	padding-top: 20px;
	margin-top: 20px;
	height: 75px;
	text-align: center;
}
#content .jingxi .jingxi-left .jingxi-box .left-middle{
	margin-top: 70px;
	text-align: center;
	color: #fff;
}
#content .jingxi .jingxi-left .jingxi-box .timelast{
	height: 63px;
	text-align: center;
	margin-top: 15px;
}
#content .jingxi .jingxi-left .jingxi-box .timelast .time1{
	font-size: 14px;
}
#content .jingxi .jingxi-left .jingxi-box .timelast .time1 span{
	font-size: 30px;
	
}
#content .jingxi .jingxi-left .jingxi-box .timelast .time1 i{
	font-style: normal;
	background: #fff;
	margin: 2px;
	border-radius: 20%;
	display: inline-block;
	padding: 4px 7px;
}
#content .jingxi .jingxi-left .thistime{
	margin-top: 20px;
	font-size: 40px;
	color: #fff;
	text-align: center;
	font-weight: bold;
}
#content .jingxi .jingxi-left .thistime span{
	width: 30px;
	height: 30px;
	display: inline-block;
	margin-left: -5px;
	background: url(../img/index-ico.png) -14px -15px no-repeat;
}
#content .jingxi .jingxi-left .left-bot{
	font-size: 20px;
	color: #fff;
	text-align: center;
	margin-top: 5px;
}
#content .jingxi .jingxi-right{
	border: 1px solid #e7e7e7;
	float: left;
	width: 870px;
	height: 458px;
	
}
#content .jingxi .jingxi-right .jxr-top{
	overflow: hidden;
	border-bottom: 1px dashed #ddd;
	padding: 5px;
	margin-left: 12px;
	margin-right: 13px;
	width: 833px;
}
#content .jingxi .jingxi-right .jxr-top .watch{
	float: left;
	width: 119px;
    height: 45px;
    color: #000;
    padding: 18px 0px 3px 0px;
    cursor: pointer;
    color: #999;
    text-align: center;
}
#content .jingxi .jingxi-right .jxr-bottom{
	overflow: hidden;
	position: relative;
	width: 100%;
}
#content .jingxi .jingxi-right .jxr-bottom .qG1{
	float: left;
	 width: 211px;
	 margin-left: 6px;
} 
#content .jingxi .jingxi-right .jxr-bottom .qG1 img{
	width: 100%;
	height: 100%;
}

#content .jingxi .jingxi-right .jxr-bottom .qG1 .qg-top,.qg-bot{
	margin: 0 20px 20px 20px;
}
#content .jingxi .jingxi-right .jxr-bottom .qG1 .qgpic{
	text-align: center;
}
#content .jingxi .jingxi-right .jxr-bottom .qG1 .qgbq{
	margin-top: 10px;
}
#content .jingxi .jingxi-right .jxr-bottom .qG1 .qgbq span{
	display: inline-block;
